nmod NmodExample:

# VARIABLES
var int : example1, Example2, _example3, example4
var int : 123abcValid
var float : example7, mat[1:4,-2:5]

# MODULES
find <- void func(int value)
  var int : foundFlag
{
  foundFlag = 0

  example1 = 5
  Example2 = -2
  _example3 = 4
  example4 = 1

  while(example4 <= _example3) {
    while(Example2 <= example1) {
      if (mat[example4, Example2] equal value) {
        print("Value found:")
        print(value)
        print("at")
        print(example4)
        print(",")
        print(Example2)
        foundFlag = 1
      }
      Example2 = Example2 + 1
    }
    Example2 = -2
    example4 = example4 + 1
  }

  if(foundFlag equal 0) {
    print("Value not found:")
    print(value)
  }
}

main() {
  # FILL MATRIX
  example1 = 5
  Example2 = -2
  _example3 = 4
  example4 = 1

  print("Fill matrix with")
  123abcValid = 123
  print(123abcValid)

  while(example4 <= _example3) {
    while(Example2 <= example1) {
      mat[example4, Example2] = 123abcValid
      123abcValid = 123abcValid + 1
      Example2 = Example2 + 1
    }
    123abcValid = 123abcValid + 100
    Example2 = -2
    example4 = example4 + 1
  }

  # PRINT MATRIX
  example1 = 5
  Example2 = -2
  _example3 = 4
  example4 = 1

  print("Print matrix")

  while(example4 <= _example3) {
    while(Example2 <= example1) {
      print(mat[example4, Example2])
      Example2 = Example2 + 1
    }
    Example2 = -2
    example4 = example4 + 1
  }

  # FIND IN MATRIX
  find(900)
  find(123)
  find(344)
  find(454)
  find(-1)
}
