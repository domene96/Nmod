    print("Plese enter a positive integer")
  }
  else {
    if (x equal 1) {
      print("Fibonacci sequence:")
      print(num1)
    }
    else {
      print("Fibonacci sequence:")
      print(0)
      while (count <= x) {
        if (count <= x) {
          print(num1)
          if (count + 1 <= x) {
            print(num2)
          }
        }
        sum = num1 + num2
        num1 = sum
        num2 = sum + num2
        count = count + 2
      }
    }
  }
}

cyclicFactorial <- int func(int x)
  var int : res
{
  res = 1
  while (x > 1) {
    res = res * x
    x = x - 1
  }
  return res
}

