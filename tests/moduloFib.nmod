# VARIABLES

fib <- void func(int x)
  var int : num1, num2, count, sum
{
  num1 = 1
  num2 = 1
  count = 1
  if (x <= 0) {
    print("Plese enter a positive integer")
  }
  else {
    if (x equal 1) {
      print("Fibonacci sequence:")
      print(num1)
    }
    else {
      print("Fibonacci sequence:")
      print(0)
      while (count <= x) {
        if (count <= x) {
          print(num1)
          if (count + 1 <= x) {
            print(num2)
          }
        }
        sum = num1 + num2
        num1 = sum
        num2 = sum + num2
        count = count + 2
      }
    }
  }
}

recursiveFib <- int func(int x) {
  if (x < 2) {
    return x
  }
  else {
    return recursiveFib(x - 1) + recursiveFib(x - 2)
  }
}

